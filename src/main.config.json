{
    "regionPrefixLookup": {
        "//": "Use the well known abbreviations from Microsoft docs.",
        "australiacentral": "auc",
        "australiaeast": "aue",
        "australiasoutheast": "ase"
    },
    "addressPrefixLookup": {
        "//": "Use a separate /16 for each region",
